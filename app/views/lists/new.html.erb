<div class="container justify-content-center">
  <div class="row card-list col-sm-10 col-lg-8  justify-content-center">
    <div class="row back-cards col-sm-10 col-lg-8 ">
      <h1>New List</h1>
        <%= simple_form_for(@list) do |f| %>
        <%= f.input :name %>
        <%= f.input :photo, as: :file %>
        <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>

